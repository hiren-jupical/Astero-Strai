<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_validate_deliveries" model="ir.cron">
        <field name="name">Cron Validate Deliveries</field>
        <field name="model_id" ref="validate_deliveries.model_sale_order"/>
        <field name="state">code</field>
        <field name="code">model._cron_validate_deliveries(200)</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">5</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(minutes=10)).strftime('%Y-%m-%d %H:%M:%S')"/>
        <field name="doall" eval="False"/>
        <field name="priority">500</field>
    </record>
</odoo>
