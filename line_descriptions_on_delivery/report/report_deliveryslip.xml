<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="report_delivery_document_inherit_descriptions" inherit_id="stock.report_delivery_document">
            <xpath expr="//table[@name='stock_move_table']/tbody" position="replace">
                 <tbody>
                    <t t-set="lines" t-value="o.move_ids.filtered(lambda x: x.product_uom_qty)"/>
                    <tr t-foreach="lines" t-as="move">
                        <td>
                            <span t-field="move.product_id"/>
                            <p t-if="move.sale_line_id.name != move.product_id.name and move.sale_line_id.name != move.product_id.display_name">
                                <span t-field="move.sale_line_id.name"/>
                            </p>
                        </td>
                        <td>
                            <span t-field="move.product_uom_qty"/>
                            <span t-field="move.product_uom"/>
                        </td>
                        <td>
                            <span t-field="move.quantity"/>
                            <span t-field="move.product_uom"/>
                        </td>
                    </tr>
                </tbody>
            </xpath>
        </template>
    </data>
</odoo>