<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_visma_salary" model="ir.cron">
        <field name="name">Cron Visma Salary Import</field>
        <field name="model_id" ref="model_cron_visma_salary"/>
        <field name="state">code</field>
        <field name="code">model._cron_visma_salary()</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
        <field name="doall" eval="False"/>
        <field name="priority">500</field>
    </record>
</odoo>
