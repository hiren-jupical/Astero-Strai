<odoo>
    <data>
        <record id="account_asset_form_strai_analytic" model="ir.ui.view">
            <field name="name">account.asset.form.inherit</field>
            <field name="model">account.asset</field>
            <field name="inherit_id" ref="account_asset.view_account_asset_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_distribution']" position="after">
                    <field name="strai_analytic_account_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>